{
  "version": 2,
  "scope": "dcad3089",
  "builds": [
    {
      "src": "app/api/index_api.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "30mb",
        "runtime": "python3.7"
      }
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/app/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "app/api/index_api.py"
    }
  ],
  "installCommand": "pip install -r requirements.txt",
  "buildCommand": "echo 'Building Python App...'",
  "outputDirectory": "app"
}